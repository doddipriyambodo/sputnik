{
    "id": "aws-deeplens-demo-squeezenet-v1.0",
    "name": "AWS Squeezenet Demo for Deeplens v1.0",
    "type": "GREENGRASS",
    "compatibility": ["aws-deeplens-no-lambda-v1.0"],
    "deviceTypeMappings": [{
        "substitute": "AWS_CAM_STREAM",
        "value": {
            "aws-deeplens-no-lambda-v1.0": "stream"
        }
    }],
    "spec": {
        "FunctionDefinitionVersion": {
            "Functions": [{
                "FunctionArn": "arn:aws:lambda:[AWS_REGION]:[AWS_ACCOUNT]:function:aws-deeplens-demo-squeezenet-python:MTM",
                "FunctionConfiguration": {
                    "EncodingType": "json",
                    "Environment": {
                        "AccessSysfs": false,
                        "ResourceAccessPolicies": [{
                                "Permission": "rw",
                                "ResourceId": "tmp"
                            },
                            {
                                "Permission": "rw",
                                "ResourceId": "mtm-model-squeezenet"
                            },
                            {
                                "Permission": "ro",
                                "ResourceId": "[AWS_CAM_STREAM]"
                            },
                            {
                                "Permission": "rw",
                                "ResourceId": "gpu"
                            }
                        ],
                        "Variables": {
                            "ML_MODEL_PATH": "/greengrass-machine-learning/mxnet/squeezenet/"
                        }
                    },
                    "MemorySize": 2097152,
                    "Pinned": true,
                    "Timeout": 25
                }
            }]
        },
        "ResourceDefinitionVersion": {
            "Resources": [{
                "Id": "mtm-model-squeezenet",
                "Name": "mtm-model-squeezenet",
                "ResourceDataContainer": {
                    "S3MachineLearningModelResourceData": {
                        "DestinationPath": "/greengrass-machine-learning/mxnet/squeezenet/",
                        "S3Uri": "[DATA_BUCKET_S3_URL]/aws-deeplens-demo-squeezenet-v1.0/models/squeezenet.zip"
                    }
                }
            }]
        },
        "SubscriptionDefinitionVersion": {
            "Subscriptions": [{
                    "Source": "arn:aws:lambda:[AWS_REGION]:[AWS_ACCOUNT]:function:aws-deeplens-demo-squeezenet-python:MTM",
                    "Subject": "mtm/[CORE]/logger",
                    "Target": "cloud"
                }, {
                    "Source": "arn:aws:lambda:[AWS_REGION]:[AWS_ACCOUNT]:function:aws-deeplens-demo-squeezenet-python:MTM",
                    "Subject": "mtm/[CORE]/camera",
                    "Target": "cloud"
                }
            ]
        }
    }
}
