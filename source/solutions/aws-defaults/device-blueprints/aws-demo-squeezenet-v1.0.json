{
    "id": "aws-demo-squeezenet-v1.0",
    "name": "AWS Squeezenet Demo v1.0",
    "type": "GREENGRASS",
    "compatibility": ["aws-deeplens-no-lambda-v1.0", "intel-up2-v0.1"],
    "deviceTypeMappings": [{
        "substitute": "CAM_STREAM_ID",
        "value": {
            "aws-deeplens-no-lambda-v1.0": "stream",
            "aws-deeplens-v1.0": "stream",
            "intel-up2-v0.1": "video0"
        }
    }, {
        "substitute": "CAM_STREAM_PERMISSIONS",
        "value": {
            "aws-deeplens-no-lambda-v1.0": "ro",
            "aws-deeplens-v1.0": "ro",
            "intel-up2-v0.1": "rw"
        }
    }, {
        "substitute": "PATH_TO_CAMERA",
        "value": {
            "aws-deeplens-no-lambda-v1.0": "awscam",
            "aws-deeplens-v1.0": "awscam",
            "intel-up2-v0.1": "/dev/video0"
        }
    }, {
        "substitute": "GPU",
        "value": {
            "aws-deeplens-no-lambda-v1.0": "gpu",
            "aws-deeplens-v1.0": "gpu",
            "intel-up2-v0.1": "gpu"
        }
    }],
    "spec": {
        "FunctionDefinitionVersion": {
            "Functions": [{
                "FunctionArn": "arn:aws:lambda:[AWS_REGION]:[AWS_ACCOUNT]:function:aws-demo-squeezenet-python:MTM",
                "FunctionConfiguration": {
                    "EncodingType": "json",
                    "Environment": {
                        "AccessSysfs": false,
                        "ResourceAccessPolicies": [{
                                "Permission": "rw",
                                "ResourceId": "tmp"
                            },
                            {
                                "Permission": "rw",
                                "ResourceId": "mtm-model-squeezenet"
                            },
                            {
                                "Permission": "[CAM_STREAM_PERMISSIONS]",
                                "ResourceId": "[CAM_STREAM_ID]"
                            },
                            {
                                "Permission": "rw",
                                "ResourceId": "[GPU]"
                            }
                        ],
                        "Variables": {
                            "ML_MODEL_PATH": "/greengrass-machine-learning/mxnet/squeezenet/",
                            "PATH_TO_CAMERA": "[PATH_TO_CAMERA]"
                        }
                    },
                    "MemorySize": 2097152,
                    "Pinned": true,
                    "Timeout": 25
                }
            }]
        },
        "ResourceDefinitionVersion": {
            "Resources": [{
                "Id": "mtm-model-squeezenet",
                "Name": "mtm-model-squeezenet",
                "ResourceDataContainer": {
                    "S3MachineLearningModelResourceData": {
                        "DestinationPath": "/greengrass-machine-learning/mxnet/squeezenet/",
                        "S3Uri": "[DATA_BUCKET_S3_URL]/aws-demo-squeezenet-v1.0/models/squeezenet.zip"
                    }
                }
            }]
        },
        "SubscriptionDefinitionVersion": {
            "Subscriptions": [{
                    "Source": "arn:aws:lambda:[AWS_REGION]:[AWS_ACCOUNT]:function:aws-demo-squeezenet-python:MTM",
                    "Subject": "mtm/[CORE]/logger",
                    "Target": "cloud"
                }, {
                    "Source": "arn:aws:lambda:[AWS_REGION]:[AWS_ACCOUNT]:function:aws-demo-squeezenet-python:MTM",
                    "Subject": "mtm/[CORE]/camera",
                    "Target": "cloud"
                }
            ]
        }
    }
}
