{
    "name": "aws-mini-connected-factory-belt-serial-node",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "dependencies": {
        "serialport": "^7.0.2"
    },
    "devDependencies": {},
    "author": "teuteuguy",
    "license": "ISC",
    "private": true,
    "scripts": {
        "prestart": "yarn install",
        "start": "node index.js",
        "build:init": "rm -rf dist && rm -rf node_modules",
        "build:zip": "zip -rq `jq -cr '.name' package.json`.zip .",
        "build:dist": "mkdir dist && mv `jq -cr '.name' package.json`.zip dist/",
        "fast-build": "rm -rf dist && yarn run build:zip && yarn run build:dist",
        "build": "yarn run build:init && yarn install --production --ignore-engines && yarn run build:zip && yarn run build:dist",
        "postinstall": "./scripts/post-install.sh"
    }
}
