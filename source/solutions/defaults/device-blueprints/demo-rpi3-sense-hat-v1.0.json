{
    "id": "demo-rpi3-sense-hat-v1.0",
    "name": "RaspberryPi 3 Sense Hat Demo v1.0",
    "type": "GREENGRASS",
    "compatibility": ["raspberry-pi-3-sense-hat-v1.0"],
    "deviceTypeMappings": [],
    "spec": {
        "FunctionDefinitionVersion": {
            "Functions": [{
                "FunctionArn": "arn:aws:lambda:[AWS_REGION]:[AWS_ACCOUNT]:function:rpi-sense-hat-display-ip-python:MTM",
                "FunctionConfiguration": {
                    "EncodingType": "json",
                    "Environment": {
                        "AccessSysfs": true,
                        "ResourceAccessPolicies": [{
                                "Permission": "rw",
                                "ResourceId": "sense-hat-led"
                            },
                            {
                                "Permission": "rw",
                                "ResourceId": "sense-hat-i2c"
                            },
                            {
                                "Permission": "ro",
                                "ResourceId": "sense-hat-event0"
                            }
                        ]
                    },
                    "MemorySize": 65536,
                    "Pinned": true,
                    "Timeout": 25
                }
            }]
        }
    }
}
